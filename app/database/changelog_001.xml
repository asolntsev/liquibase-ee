<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">


  <changeSet id="201205281433" author="Codeborne">
    <createTable tableName="account">
      <column name="id" type="NUMBER" autoIncrement="true">
        <constraints nullable="false" primaryKey="true" primaryKeyName="account_pk"/>
      </column>
      <column name="account_id" type="VARCHAR2(32 CHAR)"/>
      <column name="balance" type="NUMBER"/>
    </createTable>
  </changeSet>
</databaseChangeLog>